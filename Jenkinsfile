pipleine{
  agent any

  stages{
    stage('Build') {
      steps{
        echo 'Starting the Build stage...'
        echo "It feels great and this is my first pipleine"
      }
    }

    stage('Test') {
      steps{
        echo 'Begin Testing stage...'
      }
    }

    stage('Deploy') {
      steps{
        echo 'Initiate the Deployment stage...'
      }
    }
  }
}
